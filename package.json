{
  "name": "badge",
  "version": "0.1.0",
  "description": "A collection of HTTPS endpoints designed to provide badge metadata",
  "keywords": [
    "badge",
    "badge-generator",
    "generator",
    "service",
    "svg",
    "typescript"
  ],
  "homepage": "https://github.com/ajtorres9/badge#readme",
  "bugs": "https://github.com/ajtorres9/badge/issues",
  "license": "MIT",
  "author": "Andrew Torres <andrew.jonathan.torres@gmail.com> (https://andrewjtorr.es)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajtorres9/badge.git"
  },
  "type": "module",
  "scripts": {
    "clean": "npm-run-all --parallel \"clean:*\"",
    "clean:files": "del \"*\\.log*\"",
    "hook:pre-commit": "lint-staged",
    "lint": "npm-run-all --parallel \"lint:*\"",
    "lint:files": "prettylint \"**/*.{json,md,yml}\"",
    "lint:scripts": "eslint \"**/*.{cjs,js}\"",
    "postinstall": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/eslint": "^8.21.3",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.59.8",
    "@typescript-eslint/typescript-estree": "^5.56.0",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-circus": "^29.5.0",
    "jest-config": "^29.5.0",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.6",
    "prettylint": "^1.0.0",
    "ts-jest": "^29.0.5",
    "turbo": "^1.8.5",
    "typescript": "^5.0.2",
    "typescript-eslint-language-service": "^5.0.0"
  },
  "engines": {
    "node": ">=16.13",
    "yarn": ">=3.5"
  },
  "private": true,
  "packageManager": "yarn@3.5.0",
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "!(packages/**)*.{cjs,js}": [
      "eslint --fix"
    ],
    "!(packages/**)*.{json,md,yml}": [
      "prettylint --fix"
    ]
  }
}
